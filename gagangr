#include <stdio.h>
int main()
{
    int a,b,sum;
    printf("enter two num");
    scanf("%d%d" ,&a,&b);
    sum=a+b;
    printf("a+b=%d" ,sum);
}
#include <stdio.h>
int main()
{
    int a, b;
    printf("enter two num");
    scanf("%d%d" ,&a,&b);
    if(a>b)
    printf("a is largest number");
    else
    printf("b is largest number");
}
